#include <stdio.h>
#include <stdlib.h>

int calculate_days(int month, int year)

int main(void) {
	int q,m,k,j;
	printf("Enter Month : ");
	scanf("%d",&m);
	printf("Enter Year : ");
	scanf("%d",&q);
	if(m<=2){
		m=14-m;
		q=q-1;
	}
	k = q%100;
	j = q/100;
	q=1;
	printf("q = %d, m = %d, k = %d, j = %d",q,m,k,j);
	int ans = ( q + ((13*(m+1))/5) + k + (k/4) + (j/4) + (5*j)) % 7;
	printf("Answer is %d",ans);
	return EXIT_SUCCESS;
}
